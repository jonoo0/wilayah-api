{
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000",
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python",
        "config": {
          "installCommand": "pip install -r requirements.txt && prisma generate"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ]
  }